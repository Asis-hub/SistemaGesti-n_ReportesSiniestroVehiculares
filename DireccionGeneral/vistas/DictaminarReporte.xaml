<Window x:Class="DireccionGeneral.vistas.DictaminarReporte"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DireccionGeneral.vistas"
        mc:Ignorable="d"
        Title="Dictamen" Height="330" Width="500">
    <Grid>
        <Label Content="Dictamen:" FontSize="16" HorizontalAlignment="Left" Margin="20,15,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txt_Descripcion" FontSize="14" TextWrapping="Wrap" MaxLength="250" Margin="20,60,20,60" />

        <Button x:Name="btn_RegistrarDictamen" Content="Registrar" HorizontalAlignment="Right" Margin="0,0,110,20" FontSize="14" VerticalAlignment="Bottom" Width="75" Click="btn_RegistrarDictamen_Click" />
        <Button x:Name="btn_CancelarRegistro" Content="Cancelar" HorizontalAlignment="Right" Margin="0,0,20,20" FontSize="14" VerticalAlignment="Bottom" Width="75" RenderTransformOrigin="0.392,1.075" Click="btn_CancelarRegistro_Click" />
        

    </Grid>
</Window>
